{
  "name": "functions",
  "version": "1.0.0",
  "description": "Cloud Functions for NCP-Wheels",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "18"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "firebase-admin": "^11.9.0",
    "firebase-functions": "^4.4.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/node": "^20.0.0",
    "typescript": "^5.1.6"
  },
  "private": true
}
